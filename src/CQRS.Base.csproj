<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net9.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>disable</Nullable>
    <!-- NuGet package properties -->
    <PackageId>CQRS.Base</PackageId>
    <Title>CQRS.Base</Title>
    <Authors>malik</Authors>
    <Company>Chrak Team</Company>
    <Description>A minimal, extensible CQRS library for .NET.</Description>
    <PackageTags>CQRS;Mediator;MinimalAPI;CleanArchitecture;dotnet;asp.net core</PackageTags>
    <RepositoryUrl>https://github.com/malikmizery/CQRS.Base</RepositoryUrl>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageProjectUrl>https://github.com/malikmizery/CQRS.Base</PackageProjectUrl>
    <Version>1.0.0</Version>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Extensions.DependencyInjection" Version="9.0.7" />
  </ItemGroup>

</Project>
